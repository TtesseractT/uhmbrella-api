{
  "name": "@uhmbrella/sdk",
  "version": "0.1.0",
  "description": "Uhmbrella AIMD TypeScript SDK.",
  "license": "Apache-2.0",

  "type": "module",

  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },

  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",

  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],

  "sideEffects": false,

  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },

  "dependencies": {
    "zod": "^3.24.0"
  },

  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  }
}

